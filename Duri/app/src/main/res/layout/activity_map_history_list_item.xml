<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/activity_main"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:orientation="vertical"    android:weightSum="1"                android:background="#E6E6E6"                android:padding="15dp"    tools:context=".MapHistoryListActivity">    <FrameLayout        android:layout_width="match_parent"        android:layout_height="200dp"        android:background="@drawable/map"        android:layout_alignParentTop="true"        android:layout_alignParentStart="true">        <RelativeLayout            android:layout_width="match_parent"            android:layout_height="70dp"            android:layout_gravity="bottom"            android:background="#B3FFFFFF"            android:orientation="horizontal">            <TextView                android:id="@+id/textView4"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentStart="true"                android:layout_alignParentTop="true"                android:layout_marginStart="45dp"                android:layout_marginTop="12dp"                android:text="몽이랑 아침산책"                android:textSize="20sp" />            <TextView                android:id="@+id/textView5"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignStart="@+id/textView4"                android:layout_below="@+id/textView4"                android:text="아침산책" />            <TextView                android:id="@+id/textView6"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentEnd="true"                android:layout_marginEnd="17dp"                android:text="1,283"                android:layout_alignTop="@+id/imageView4"/>            <TextView                android:text="39분"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:id="@+id/textView10"                android:layout_marginTop="5dp"                android:layout_alignTop="@+id/textView4"                android:layout_alignEnd="@+id/textView6"/>            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                app:srcCompat="@drawable/icon_walk"                android:id="@+id/imageView4"                android:layout_below="@+id/textView4"                android:layout_toStartOf="@+id/textView6"/>            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                app:srcCompat="@drawable/img_time_list"                android:id="@+id/imageView6"                android:layout_alignBottom="@+id/textView10"                android:layout_alignStart="@+id/imageView4"/>        </RelativeLayout>    </FrameLayout></RelativeLayout>